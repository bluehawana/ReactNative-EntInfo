{
  "expo": {
    "name": "EntInfo",
    "slug": "EntInfo",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#0F171E"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "CFBundleURLTypes": [
          {
            "CFBundleURLSchemes": [
              "entinfo"
            ]
          },
          {
            "CFBundleURLSchemes": [
              "entinfo"
            ]
          }
        ]
      },
      "bundleIdentifier": "com.anonymous.EntInfo"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#0F171E"
      },
      "package": "com.anonymous.EntInfo",
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "entinfo"
            }
          ],
          "category": [
            "DEFAULT",
            "BROWSABLE"
          ]
        }
      ]
    }
  }
}
